<div class="col-md-12">
  <h2 class="text-center"> 我加入的讨论版 </h2>

  <table class="table">
    <thead>
      <tr>
        <th>#</th>
        <th> Title </th>
        <th> Description </th>
        <th> Post count </th>
        <th> Last update </th>

      </tr>
    </thead>

    <tbody>
      <% @moviedataas.each do |moviedataa| %>
      <tr>
        <td>#</td>
        <td> <%= link_to(moviedataa.title, moviedataa_path(moviedataa)) %></td>
        <td> <%= moviedataa.description %></td>
        <td> <%= moviedataa.posts.count %> </td>
        <td> <%= moviedataa.updated_at %> </td>
      </tr>
      <% end %>
      
    </tbody>
  </table>
</div>
